{
  "name": "multi-clinic-management-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de gerenciamento de multi clínicas odontológicas e estéticas",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "generate-secret": "node -e \"console.log('JWT_SECRET=' + require('crypto').randomBytes(32).toString('hex'))\"",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "seed": "node scripts/sync-database.js",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "prestart": "node config/generate-config.js",
    "predev": "node config/generate-config.js",
    "test": "jest --watchAll --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "petshop",
    "backend",
    "nodejs",
    "express",
    "postgresql"
  ],
  "author": "Gabriele de Assis Silva Carvalho <gabi.05assis9@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
